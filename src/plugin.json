{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Prtg",
  "id": "maxmarkusprogram-prtg-datasource",
  "metrics": true,
  "info": {
    "description": "PRTG Network Monitor data source",
    "author": {
      "name": "Maxmarkusprogram",
      "email": "mustafa.ozdemir1408@gmail.com", 
      "url": "https://github.com/1DeliDolu/prtg-grafana-datasource.git"
    },
    "keywords": ["datasource,prtg"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "annotations": true,
    "links": [
      {
        "name": "GitHub",
        "url": "https://github.com/1DeliDolu/prtg-grafana-datasource"
      }
    ],
    "screenshots": [
      {
        "name": "PRTG Datasource",
        "path": "img/1737448469409.png"
      }
    ],
    "version": "1.0.0",
    "updated": "2024-02-14"
  },
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
  },
  "routes": [
    {
      "path": "hostname",
      "url": "https://{{ .JsonData.hostname }}/api/",
      "body": [
        {
      "username": "{{ .JsonData.username }}",
      "passhash": "{{ .SecureJsonData.passhash }}"
        }
        
      ]
    }
  ]
}
